        "items": [
          {
            "comment": {
              "brief": "Number of columns to lay out.",
              "full": "Number of columns to lay out.\n\n    <Panel Color=\"Black\" >\n        <!-- Lay out lots of yellow circles and red rectangles in columns -->\n        <ColumnLayout ColumnCount=\"10\" />\n        <Each Count=\"70\">\n            <Circle Margin=\"5\" Width=\"10\" Height=\"10\" Color=\"Yellow\" />\n            <Rectangle Margin=\"5\" Width=\"10\" Height=\"40\" Color=\"Red\" />\n        </Each>\n    </Panel>"
            },
            "returns": {
              "href": "uno/int",
              "title": "int",
              "fullyQualifiedTitle": "int"
            },
            "id": {
              "id": "fuse/layouts/columnlayout/columncount",
              "parentId": "fuse/layouts/columnlayout",
              "type": "UxProperty",
              "modifiers": [
                "public"
              ]
            },
            "uri": {
