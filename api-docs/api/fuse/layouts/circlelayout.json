    "titles": {
      "pageTitle": "CircleLayout Class",
      "navigationTitle": "CircleLayout Class",
      "fullTitle": "Fuse.Layouts.CircleLayout",
      "indexTitle": "CircleLayout",
      "fullyQualifiedIndexTitle": "Fuse.Layouts.CircleLayout"
